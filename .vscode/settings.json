{
    "i18n-ally.localesPaths": ["src/i18n", "src/i18n/locales"],
    "i18n-ally.keystyle": "nested",
    "i18n-ally.sourceLanguage": "zh-cn",
    "emeraldwalk.runonsave": {
        "message": "🚀🚀🚀 Start Build 🚀🚀🚀",
        "messageAfter": "✌️✌️✌️ Build Complete ✌️✌️✌️",
        "commands": [
            {
                // Run whenever any file is saved
                "match": ".*",
                "cmd": "pnpm build"
            }
        ]
    }
}
